{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "strictNullChecks": true,
        "allowJs": true,
        "plugins": [
            {
                "name": "@astrojs/ts-plugin"
            },
        ],
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
        },
        "composite": true
    }
}